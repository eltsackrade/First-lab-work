package com.company;
import java.util.Scanner;



public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

      int b;
      int a;
      a=input.nextInt();
      int c=0;
      for (int i=1; i<=a; i++)
      {
          b=input.nextInt();
          if (b==0);
          c++;

      }
      System.out.println(c);
    }
}
