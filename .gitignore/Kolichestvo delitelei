package com.company;
import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x;
        int sum;
        sum=0;
        x=input.nextInt();
        for (int i=1; i<=x; i++)
        {
            if (x%i==0)
            {
                sum++;
            }
        }
        System.out.println(sum);



    }
}
