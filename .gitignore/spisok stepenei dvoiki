package com.company;
        import java.util.Scanner;



public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        int x;
        x=input.nextInt();
        int n=1;
        while (n<=x)
        {
            System.out.println(n);
            n*=2;


        }

    }
}
