package com.company;
import java.util.Scanner;


public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int x;
        int sum;
        sum=0;
        
        for (int i=1; i<=100; i++)
        {
            x=input.nextInt();
            sum=sum+x;
        }
        System.out.println(sum);



    }
}
