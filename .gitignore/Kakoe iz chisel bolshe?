package com.company;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a;
        int b;
        a=input.nextInt();
        b=input.nextInt();
        if (a>b)
        {
            System.out.println("1");
        }
        if (a<b)
        {
            System.out.println("2");
        }
        if (a==b)
        {
            System.out.println("0");
        }


    }
}
