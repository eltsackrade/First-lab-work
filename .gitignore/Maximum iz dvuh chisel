import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a;
        int b;
        a=input.nextInt();
        b=input.nextInt();
        if (a>b) {
            System.out.println(a);
        }
        else if (b>a)
        {
            System.out.println(b);
        }


    }
}
